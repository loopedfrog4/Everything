    "def is_leap(year):\n",
    "    # Checking if the given year is leap year  \n",
    "    if((year % 400 == 0) or  (year % 100 != 0) and  (year % 4 == 0)):   \n",
    "        return True\n",
    "    # Else it is not a leap year  \n",
    "    else:  \n",
    "        return False\n",
    "    \n",
    "\n",
    "year = int(input())\n",
    "print(is_leap(year))"
   ]
  }
