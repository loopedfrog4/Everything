    "# default fibonacci\n",
    "def fib(n):\n",
    "    if n==0:\n",
    "        return 0\n",
    "    elif n==1:\n",
    "        return 1\n",
    "    elif n >= 2:\n",
    "        return fib(n-1) + fib(n-2)\n",
    "\n",
    "print(fib(5))"
   ]
